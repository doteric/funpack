{
  "type": "module",
  "scripts": {
    "prepackage": "tsc --noEmit",
    "package": "funpack"
  },
  "funpack": {
    "settings": {
      "packageFieldsToCopy": [
        "version",
        "type"
      ],
      "zip": true
    },
    "functions": {
      "main": "./src/main.ts"
    }
  },
  "dependencies": {
    "resolve-package-path": "^4.0.3"
  },
  "devDependencies": {
    "funpack": "file:../.."
  }
}
